{
  "name": "max-flow-scheduler",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "dev": "pnpm -r --parallel run dev --if-present",
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test && pnpm test:engine-cpp",
    "typecheck": "pnpm -r run typecheck",
    "format": "pnpm -r run format",
    "test:engine-cpp": "bash scripts/run-engine-tests.sh",
    "bench:engine-cpp": "node scripts/benchmark-engine.mjs",
    "bench:engine-cpp:check": "node scripts/check-engine-benchmark.mjs",
    "ci": "pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.0.9"
  }
}
